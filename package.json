{
    "name": "miliband",
    "version": "1.0.0",
    "description": "Miliband Wordpress Theme",
    "main": "index.js",
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JacobWeinbren/miliband.git"
    },
    "keywords": [
        "custom-components",
        "wordpress",
        "theme"
    ],
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "phpVersion": "5.4"
    },
    "browserslist": [
        "> 1%",
        "not dead",
        "last 2 versions"
    ],
    "author": "Jacob",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "^7.15.8",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "autoprefixer": "^10.3.7",
        "esbuild": "^0.13.4",
        "eslint-config-prettier": "^8.3.0",
        "postcss": "^8.3.9",
        "rollup": "^2.58.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-esbuild": "^4.5.0",
        "rollup-plugin-execute": "^1.1.1",
        "rollup-plugin-postcss": "^4.0.1",
        "rollup-plugin-terser": "^7.0.2",
        "sass": "^1.42.1",
        "typescript": "^4.4.4"
    },
    "devDependencies": {
        "@prettier/plugin-php": "^0.17.5",
        "prettier": "^2.4.1"
    }
}
